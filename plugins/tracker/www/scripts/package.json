{
  "author": "Enalean Team",
  "name": "tuleap-tracker",
  "version": "0.2.0",
  "homepage": "https://www.enalean.com",
  "description": "Tuleap Tracker plugin",
  "license": "GPL-2.0+",
  "private": true,
  "dependencies": {
    "angular": "^1.3.20",
    "angular-filter": "^0.5.16",
    "angular-gettext": "^1.1.4",
    "angular-moment": "^0.8.3",
    "angular-sanitize": "^1.3.20",
    "codemirror": "5.22.0",
    "d3-array": "^1.2.1",
    "d3-axis": "^1.0.8",
    "d3-scale": "^1.0.7",
    "d3-selection": "^1.2.0",
    "d3-shape": "^1.2.0",
    "imports-loader": "^0.7.1",
    "lodash": "2.4.0",
    "moment": "^2.18.1",
    "ng-ckeditor": "^2.0.3",
    "node-gettext": "^2.0.0",
    "pump": "^3.0.0",
    "readable-stream": "^2.3.6",
    "sprintf-js": "^1.1.1",
    "vue": "^2.5.13",
    "vue-gettext": "^2.1.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.3.20",
    "gulp": "^3.9.1",
    "gulp-angular-gettext": "^2.2.0"
  },
  "config": {
    "bin": "../../../../node_modules/.bin/"
  },
  "scripts": {
    "build": "NODE_ENV=production $npm_package_config_bin/webpack --mode=production",
    "coverage-modal": "rm -rf ./coverage/* && NODE_ENV=coverage $npm_package_config_bin/karma start ./karma.conf.js && $npm_package_config_bin/http-server -os -a 0.0.0.0 -p 9000 coverage/",
    "coverage-artifacts": "rm -rf ./coverage/* && NODE_ENV=coverage $npm_package_config_bin/karma start ./artifact-action-buttons/karma.conf.js && $npm_package_config_bin/http-server -os -a 0.0.0.0 -p 9000 coverage/",
    "test": "NODE_ENV=test $npm_package_config_bin/karma start ./karma.conf.js && NODE_ENV=test $npm_package_config_bin/karma start ./artifact-action-buttons/karma.conf.js",
    "watch-modal": "NODE_ENV=watch $npm_package_config_bin/concurrently --raw --kill-others 'gulp watch' '$npm_package_config_bin/webpack --watch --mode=development' '$npm_package_config_bin/karma start ./karma.conf.js'",
    "watch-artifacts": "NODE_ENV=watch $npm_package_config_bin/concurrently --raw --kill-others '$npm_package_config_bin/webpack --watch --mode=development' '$npm_package_config_bin/karma start ./artifact-action-buttons/karma.conf.js'",
    "extract-gettext-cli": "$npm_package_config_bin/gettext-extract --attribute v-translate $(find artifact-action-buttons/ -type f -name '*.vue')"
  }
}
